          "keyword": { "color": "#990005" },
          "number": { "color": "#73325b" },
          "operator": { "color": "#2b513a" },
          "property": { "color": "#055160" },
          "punctuation": { "color": "#3c3836" },
          "string": { "color": "#4e4b09" },
