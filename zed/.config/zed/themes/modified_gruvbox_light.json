{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Gruvbox Light Soft (7:1 High Contrast)",
  "author": "b-3po / programmatically adjusted",
  "themes": [
    {
      "name": "Gruvbox Light Soft (7:1 High Contrast)",
      "appearance": "light",
      "style": {
        "editor.background": "#f2e5bc",
        "editor.foreground": "#282828",
        "syntax": {
          "attribute": { "color": "#085160" },
          "boolean": { "color": "#73325b" },
          "comment": { "color": "#504841" },
          "comment.doc": { "color": "#4f4843" },
          "constant": { "color": "#65420a" },
